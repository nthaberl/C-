@model List<Dish>;
@using CRUDelicious.Models;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>
        Welcome to CRUDelicious
    </h1>

    <h3>Check out some recent dishes!</h3>
    @foreach(Dish dish in Model)
    {
        <p><a href="/@dish.DishId">@dish.Name</a> created by @dish.Chef</p>
    }

    <h3>Feel like you've got a good recipe? Add a dish <a href="/new">here</a></h3>
</body>
</html>